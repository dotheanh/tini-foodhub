<view class="wrap {{fixedHeader ? 'fix-header' : ''}}">

  <view class="tab-select">
    <custom-top-tabs activeTab="{{activeTab}}">
    </custom-top-tabs>
  </view>

  <view class="posts-list">
    <view tiki:for="{{posts}}" tiki:for-item="post">
      <post-surfing post="{{post}}" isLoading="{{isLoading}}">
      </post-surfing>
    </view>
  </view>

</view>